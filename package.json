{
  "name": "elaina-steam-theme",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:dev": "rollup -c",
    "clean": "rimraf dist",
    "dev:link": "powershell -ExecutionPolicy Bypass -File ./scripts/symlink.ps1 -Action link",
    "dev:unlink": "powershell -ExecutionPolicy Bypass -File ./scripts/symlink.ps1 -Action unlink",
    "dev:relink": "powershell -ExecutionPolicy Bypass -File ./scripts/symlink.ps1 -Action relink",
    "compile": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsc --noEmit",
    "lint": "eslint ./src",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint ./src --fix",
    "fix:prettier": "prettier --write ./src",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "neostandard": "^0.12.2",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-rem-to-pixel": "^4.1.2",
    "postcss-url": "^10.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.94.2",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=22"
  }
}
